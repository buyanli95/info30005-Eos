<!--Home page for participant, after login-->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Eos-Participant</title>
    <script src="/js/goBack.js"></script>
    <link rel="stylesheet" href="/css/participant_home_topbar.css">
